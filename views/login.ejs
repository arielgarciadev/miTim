<div id="divbody" class="text-center">
  <form class="form-signin" action="/users/login" method="POST">
    <img class="mb-4" src="../public/logo.png" alt="" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">Login</h1>
    <% include ./partials/messages %>
    <label for="inputEmail" class="sr-only">Correo</label>
    <input type="email" id="email" name="email" class="form-control" placeholder="Correo" required="" autofocus="">
    <label for="inputPassword" class="sr-only">Contraseña</label>
    <input type="password" name="password" id="password" class="form-control" placeholder="Contraseña" required="">
    <div class="checkbox mb-3">
    </div>
    <button class="btn btn-lg btn-outline-success btn-block" type="submit">Ingresar</button>
    <p class="lead mt-4">
      ¿No tienes una cuenta? <a href="/users/register">Registráte</a>
    </p>
  </form>
</div>